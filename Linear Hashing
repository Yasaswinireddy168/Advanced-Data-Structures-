#include<stdio.h>
int main() {
	int hashtable[10];
	int key[]={23,43,13,27,39};
	int i,n=5;
	for(i=0;i<10;i++) 
	hashtable[i]=-1;
	for(i=0;i<n;i++)
	 {
	 	int index=key[i]%10;
		while(hashtable[index]!=-1) {
			index=(index+1)%10;
		}
		hashtable[index]=key[i];
	}
	printf("\nHash Table:\n");
	for(i=0;i<10;i++) {
		printf("%d-->%d\n",i,hashtable[i]);
	}
}
