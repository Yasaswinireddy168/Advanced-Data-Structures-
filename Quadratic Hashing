#include<stdio.h>
#define SIZE 10
int hashTable[SIZE];
int main() {
	int i,key,index,c=0,n;
	for(i=0;i<SIZE;i++) 
	hashTable[i]=-1;
	printf("Enter number of elements to insert:");
	scanf("%d",&n);
	for(i=0;i<n;i++) {
		printf("Enter key:");
		scanf("%d",&key);
		index=key%SIZE;
		c=0;
		while(hashTable[(index+c*c)%SIZE]!=-1) {
			c++;
			
		}
		hashTable[(index+c*c)%SIZE]=key;
	}
	printf("\nHash Table:\n");
	for(i=0;i<SIZE;i++) {
		if(hashTable[i]==-1) {
			printf("index %d:Empty\n",i);
		}
			else
			{
			printf("index %d:%d\n",i,hashTable[i]);
			
	}
	}
}
